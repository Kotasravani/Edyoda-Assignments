   "source": [
    "tup = list(tuple(map(int,input().split())) \n",
    "         for r in range(int(input('enter no of rows : '))))  \n",
    "print(\"The list is:\",tup) \n",
    "\n",
    "def last(n): \n",
    "    return n[-1]\n",
    "\n",
    "def sort_list_last(tup):\n",
    "    return sorted(tup, key = last)\n",
    "\n",
    "print(\"The output is:\",sort_list_last(tup))\n"
   ]
  }
